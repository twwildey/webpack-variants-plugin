{
  "name": "@amzn/webpack-variants",
  "version": "0.1.0",
  "description": "Package for building asset variants using webpack",
  "author": "Todd Wildey <twwildey@amazon.com>",
  "license": "UNLICENSED",
  "repository": "ssh://git.amazon.com/pkg/NodeJS-webpack-variants",
  "homepage": "https://code.amazon.com/packages/NodeJS-webpack-variants",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "babel": "babel src --source-maps",
    "build:esm": "BABEL_ENV=esm npm run babel -- --out-dir dist/esm --out-file-extension '.mjs'",
    "build:cjs": "BABEL_ENV=cjs npm run babel -- --out-dir dist/cjs --out-file-extension '.cjs'",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "cd test/ && ./run_tests.sh",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "dist"
  ],
  "npm-pretty-much": {
    "runTest": "never",
    "ciBuild": "always"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-syntax-class-properties": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/polyfill": "^7.8.5",
    "@babel/preset-env": "^7.8.0",
    "@babel/register": "^7.8.0",
    "@babel/runtime": "^7.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0"
  }
}
